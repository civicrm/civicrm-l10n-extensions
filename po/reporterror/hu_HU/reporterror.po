#: CRM/Admin/Form/Setting/ReportError.php
msgid "Error Report Recipient"
msgstr "Hibajelentés címzett"

#: CRM/Admin/Form/Setting/ReportError.php
msgid "Display a full backtrace in e-mails?"
msgstr "Megjelenítse a teljes visszakövetést e-mailekben?"

#: CRM/Admin/Form/Setting/ReportError.php
msgid "Display POST data in e-mails?"
msgstr "Megjelenítse a POST adatokat e-mailekben?"

#: CRM/Admin/Form/Setting/ReportError.php
msgid "-Select-"
msgstr "-Válassza ki-"

#: CRM/Admin/Form/Setting/ReportError.php
msgid "Do nothing (show the CiviCRM error)"
msgstr "Ne tegyen semmit (mutassa a CiviCRM hibát)"

#: CRM/Admin/Form/Setting/ReportError.php
msgid "Redirect to front page of CMS"
msgstr "A tartalom kezelő rendszer címlapjára átirányítás"

#: CRM/Admin/Form/Setting/ReportError.php
msgid "Redirect to a specific contribution page"
msgstr "Egyedi adomány oldalra átirányítás"

#: CRM/Admin/Form/Setting/ReportError.php
msgid "Enable transparent redirection?"
msgstr "Engedélyezi az átlátható átirányítást?"

#: CRM/Admin/Form/Setting/ReportError.php
msgid "Send error reports for this particular error?"
msgstr "Küldjön hibaüzenetet erről az egyedi hibáról?"

#: CRM/Admin/Form/Setting/ReportError.php
msgid "Redirect to Contribution Page"
msgstr "Adomány oldalra átirányítás"

#: CRM/Admin/Form/Setting/ReportError.php
msgid "Save"
msgstr "Mentés"

#: CRM/Admin/Form/Setting/ReportError.php
msgid "Cancel"
msgstr "Mégsem"

#: CRM/Admin/Form/Setting/ReportError.php
msgid "Settings saved."
msgstr "Beállítások elmentve."

#: reporterror.php
msgid "CiviCRM Error Report was uninstalled"
msgstr "CiviCRM Hibaüzenet kiterjesztés eltávolítva"

#: reporterror.php
msgid "CiviCRM error [redirected] at %1"
msgstr "CiviCRM hiba %1 [átirányítva] "

#: reporterror.php
msgid "CiviCRM error at %1"
msgstr "CiviCRM hiba %1 "

#: reporterror.php
msgid "There was a CiviCRM error at %1."
msgstr "CiviCRM hiba történt %1."

#: reporterror.php
msgid "Date: %1"
msgstr "Dátum: %1"

#: reporterror.php
msgid "Error handling rules redirected the user to:"
msgstr "Hiba kezelési szabályok átirányítva a felhasználőhoz:"

#: templates/CRM/Admin/Form/Setting/ReportError.tpl
msgid "General Setup"
msgstr "Általános beállítások"

#: templates/CRM/Admin/Form/Setting/ReportError.tpl
msgid ""
"This usually is your domain administrator's email. Separate multiple email "
"addresses with a comma (','). If left empty, no e-mails will be sent."
msgstr "Ez általában a weboldala adminisztrátorának az e-mailje. Válassza el az e-mail címeket vesszővel (','). Ha üresen hagyja, e-mail nem kerül elküldésre."

#: templates/CRM/Admin/Form/Setting/ReportError.tpl
msgid "Contribution pages with no referrer"
msgstr "Hivatkozás nélküli adomány oldalak"
