#: CRM/Core/Payment/Stripe.php
msgid "Stripe"
msgstr "Stripe"

#: CRM/Core/Payment/Stripe.php
msgid "The \"Secret Key\" is not set in the Stripe Payment Processor settings."
msgstr "La \"Clave Secreta\" no está definida en el administrador del procesador de pago de Stripe."

#: CRM/Core/Payment/Stripe.php
msgid ""
"The \"Publishable Key\" is not set in the Stripe Payment Processor settings."
msgstr "La \"Clave Pública\" no está definida en el administrador del procesador de pago de Stripe."

#: CRM/Core/Payment/Stripe.php
msgid "- select -"
msgstr "- seleccionar -"

#: CRM/Core/Payment/Stripe.php
msgid "Card Number"
msgstr "Número de tarjeta"

#: CRM/Core/Payment/Stripe.php
msgid "Security Code"
msgstr "Código de seguridad"

#: CRM/Core/Payment/Stripe.php
msgid "Expiration Date"
msgstr "Fecha de vencimiento"

